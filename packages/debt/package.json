  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.76.0",
    "@types/node": "^11.11.6",
    "@types/sinon": "^7.0.9",
    "ava": "^1.3.1",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.0",
    "supertest": "^4.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  }
