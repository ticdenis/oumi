{
  "name": "@oumi-package/core",
  "description": "This package provides the basic units needed for the Oumi architecture.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "clean": "rimraf lib",
    "build": "tsc --declaration",
    "build:watch": "tsc --declaration -w",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch",
    "coverage": "nyc ava --verbose",
    "report": "nyc report -r=text-lcov > coverage.lcov"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.76.0",
    "@types/node": "^11.11.0",
    "@types/sinon": "^7.0.9",
    "ava": "^1.3.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "pm2": "^3.3.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.7",
    "supertest": "^4.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/ramda": "^0.26.3",
    "@types/uuid": "^3.4.4",
    "io-ts": "^1.8.3",
    "io-ts-types": "^0.4.4",
    "monocle-ts": "^1.7.1",
    "ramda": "^0.26.1",
    "uuid": "^3.3.2"
  }
}
