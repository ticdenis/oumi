{
  "name": "@oumi-package/shared",
  "description": "This package provides the shared componentes between Oumi packages.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "clean": "rimraf lib",
    "build": "tsc --declaration",
    "watch": "tsc --declaration -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "upgrade": "yarn upgrade --latest",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch",
    "coverage": "nyc ava --verbose",
    "report": "nyc report -r=text-lcov > coverage.lcov"
  },
  "dependencies": {
    "fp-ts": "^1.17.0",
    "io-ts": "^1.8.3",
    "io-ts-types": "^0.4.7",
    "monocle-ts": "^1.7.1",
    "ramda": "^0.26.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.87.0",
    "@types/node": "^11.13.0",
    "@types/ramda": "^0.26.7",
    "@types/sinon": "^7.0.11",
    "@types/uuid": "^3.4.4",
    "ava": "^1.4.1",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  }
}
