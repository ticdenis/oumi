{
  "name": "@oumi-package/user",
  "description": "This package provides the way to operate with Oumi users.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/main.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "clean": "rimraf dist",
    "build": "tsc --declaration",
    "build:watch": "tsc --declaration -w",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch",
    "coverage": "nyc ava --verbose",
    "report": "nyc report -r=text-lcov > coverage.lcov"
  },
  "dependencies": {
    "@oumi-package/shared": "0.0.0",
    "fp-ts": "^1.14.3"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.76.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.0",
    "@types/sinon": "^7.0.9",
    "ava": "^1.3.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "pm2": "^3.3.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.7",
    "supertest": "^4.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  }
}
