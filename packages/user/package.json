{
  "name": "@oumi-package/user",
  "description": "This package provides the way to operate with Oumi users.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "types": "./lib/index.d.ts",
  "scripts": {
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "clean": "rimraf lib",
    "build": "tsc --declaration",
    "watch": "tsc --declaration -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "upgrade": "yarn upgrade --latest",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch",
    "coverage": "nyc ava --verbose",
    "report": "nyc report -r=text-lcov > coverage.lcov"
  },
  "dependencies": {
    "@oumi-package/shared": "0.0.0",
    "bcrypt": "^3.0.4",
    "fp-ts": " ^1.15.1",
    "io-ts": "^1.8.3",
    "jwt-simple": "^0.5.5",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.87.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.0",
    "@types/ramda": "^0.26.8",
    "@types/sinon": "^7.0.11",
    "ava": "^1.4.1",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
}
