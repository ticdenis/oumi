{
  "name": "@oumi-package/user",
  "description": "This package provides the way to operate with Oumi users.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "types": "./lib/index.d.ts",
  "scripts": {
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "clean": "rimraf lib",
    "build": "tsc --declaration",
    "build:watch": "tsc --declaration -w",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch",
    "coverage": "nyc ava --verbose",
    "report": "nyc report -r=text-lcov > coverage.lcov"
  },
  "dependencies": {
    "@oumi-package/core": "0.0.0",
    "bcrypt": "^3.0.4",
    "fp-ts": "^1.14.3",
    "io-ts": "^1.8.3",
    "jwt-simple": "^0.5.5",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.76.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.6",
    "@types/sinon": "^7.0.9",
    "ava": "^1.3.1",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.0",
    "supertest": "^4.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  }
}
