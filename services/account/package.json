{
  "name": "@oumi-service/account",
  "description": "This service provides the way to operate with Oumi accounts.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "pm2 start --name '@oumi-service/account' ts-node -- -P tsconfig.json ./src/main.ts",
    "serve": "node dist/main.js",
    "build": "tsc --declaration",
    "watch": "tsc --declaration -w",
    "debug": "nodemon --delay 3 --watch ./dist --legacy-watch ./dist/main.js",
    "dev": "concurrently --kill-others \"yarn debug\" \"yarn watch\"",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --coverage",
    "report": "jest --coverage",
    "migrate": "./tools/migrate.ts"
  },
  "dependencies": {
    "@oumi-package/core": "0.0.0",
    "@oumi-package/user": "0.0.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "http-status-codes": "^1.3.2",
    "inversify": "^5.0.1",
    "io-ts": "^1.8.3",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.6",
    "typeorm": "^0.2.15",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.76.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.0",
    "@types/shelljs": "^0.8.3",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "pm2": "^3.4.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  }
}
