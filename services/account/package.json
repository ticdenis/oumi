{
  "name": "@oumi-service/account",
  "description": "This service provides the way to operate with Oumi accounts.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/main.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "clean": "rimraf dist",
    "start": "pm2 start --name '@oumi-service/account' ts-node -- -P tsconfig.json ./src/main.ts",
    "serve": "node dist/main.js",
    "build": "tsc --declaration",
    "watch": "tsc --declaration build -w",
    "debug": "nodemon --delay 3 --watch ./dist --legacy-watch ./dist/main.js",
    "dev": "concurrently --kill-others \"yarn debug\" \"yarn watch\"",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --coverage",
    "report": "jest --coverage"
  },
  "dependencies": {
    "@oumi-package/shared": "0.0.0",
    "@oumi-package/user": "0.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "http-status-codes": "^1.3.2",
    "inversify": "^5.0.1",
    "io-ts": "^1.8.2",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.76.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.0",
    "@types/sinon": "^7.0.9",
    "concurrently": "^4.1.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "pm2": "^3.3.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.7",
    "supertest": "^4.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  }
}
