{
  "name": "@oumi-service/transactions",
  "description": "This service provides the way to operate with Oumi debts and payments.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node dist/main.js",
    "serve": "pm2 start --name '@oumi-service/transactions' ts-node -- -P tsconfig.json ./src/main.ts --watch",
    "serve:logs": "pm2 logs @oumi-service/transactions",
    "serve:down": "pm2 delete @oumi-service/transactions",
    "build": "tsc --declaration",
    "watch": "tsc --declaration -w",
    "debug": "nodemon --watch ./dist --legacy-watch ./dist/main.js",
    "dev": "concurrently --kill-others \"yarn debug\" \"yarn watch\"",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "upgrade": "yarn upgrade --latest",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --coverage",
    "report": "jest --coverage",
    "migrate": "./tools/migrate.ts",
    "pm2": "pm2"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.1.0",
    "@oumi-package/debt": "0.0.0",
    "@oumi-package/shared": "0.0.0",
    "@oumi-package/user": "0.0.0",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "fp-ts": "^1.17.1",
    "graphql": "^14.2.1",
    "helmet": "^3.16.0",
    "http-status-codes": "^1.3.2",
    "inversify": "^5.0.1",
    "io-ts": "^1.8.5",
    "io-ts-types": "^0.4.7",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.10.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.6",
    "typeorm": "^0.2.16",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.87.0",
    "@types/compression": "^0.0.36",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/shelljs": "^0.8.4",
    "@types/supertest": "^2.0.7",
    "apollo-server-testing": "^2.4.8",
    "concurrently": "^4.1.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "pm2": "^3.5.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
}
