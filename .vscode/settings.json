{
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/lib": true,
    "**/dist": true,
    "**/var": true
  },
  "editor.formatOnSave": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "sqltools.connections": [{
      "database": "${workspaceFolder}/services/accounts/var/db-accounts.sqlite",
      "dialect": "SQLite",
      "name": "oumi-accounts"
    },
    {
      "database": "${workspaceFolder}/services/transactions/var/db-transactions.sqlite",
      "dialect": "SQLite",
      "name": "oumi-transactions"
    }
  ]
}
