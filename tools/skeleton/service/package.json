{
  "name": "@oumi-service/{{name}}",
  "description": "{{description}}",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/main.js",
  "scripts": {
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Use yarn\")'",
    "clean": "rimraf dist",
    "start": "pm2 start --name '@oumi-service/{{name}}' ts-node -- -P tsconfig.json ./src/main.ts",
    "serve": "node dist/main.js",
    "build": "tsc --declaration",
    "watch": "tsc --declaration -w",
    "debug": "nodemon --delay 3 --watch ./dist --legacy-watch ./dist/main.js",
    "dev": "concurrently --kill-others \"yarn debug\" \"yarn watch\"",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettier": "prettier --config .prettierrc --write \"./**/*.ts\"",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --coverage",
    "report": "jest --coverage"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.76.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.0",
    "@types/shelljs": "^0.8.3",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "pm2": "^3.4.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  }
}
